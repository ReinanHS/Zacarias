{
    "name": "ReinanHS/Zacarias",
    "description": "The Zacarias PHP framework",
    "keywords": [
        "framework",
        "mvc",
        "psr-4"
    ],
    "license": "MIT",
    "homepage": "https://reinanhs.github.io/zacarias/",
    "type": "project",
    "require": {
        "php": ">=7.0.0",
        "psr/log": "^1.0",
        "twig/twig": "^2.0"
    },
    "autoload": {
        "classmap": [],
        "psr-4": {
            "Zacarias\\": "src/",
            "Bootstrap\\": "config/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    }
}